{
  "name": "shape-2-flat",
  "version": "0.1.0",
  "description": "Convert an SVG path into a printable 2D net of an extruded prism",
  "bin": {
    "shape-2-flat": "bin/shape-2-flat.js"
  },
  "main": "src/core.mjs",
  "scripts": {
    "start": "node bin/shape-2-flat.js",
    "dev": "node bin/shape-2-flat.js --help",
    "test": "node test/path-segments.test.mjs && node test/shape-glue-tabs.test.mjs && node test/geometry.test.mjs",
    "test:segments": "node test/path-segments.test.mjs",
    "test:tabs": "node test/shape-glue-tabs.test.mjs",
    "test:quick": "node test/quick-test.mjs"
  },
  "keywords": [
    "svg",
    "path",
    "net",
    "papercraft",
    "extrude",
    "prism"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "svg-path-properties": "^1.0.16",
    "@xmldom/xmldom": "^0.8.10",
    "svgpath": "^2.6.0",
    "xpath": "^0.0.34",
    "yargs": "^17.7.2"
  }
}
